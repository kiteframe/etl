{
  "name": "kiteframe/etl",
  "description": "A package for building ETL processes in PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "kiteframe",
      "email": "contact@kiteframe.co.uk"
    }
  ],
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpstan/phpstan": "^0.12.18",
    "phpstan/phpstan-phpunit": "^0.12.6",
    "phpstan/phpstan-strict-rules": "^0.12.2",
    "phpunit/phpunit": "^9",
    "sensiolabs-de/deptrac-shim": "^0.6.0",
    "sensiolabs/security-checker": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "kiteframe\\etl\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "kiteframe\\etl\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "prefer-stable": true
}
